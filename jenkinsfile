pipeline{
    agent any
    tools{
        maven 'maven'
    }
    stages{
        stage('Checkout'){
                steps{
                    git branch: 'main', url:'https://github.com/nitish-002/cicdPractice.git'
                }
        }

        stage('Build'){
            steps{
                sh 'mvn clean package'
            }
        }
        steps('Test'){
            steps{
                sh 'mvn test'
            }
        }
    }
}